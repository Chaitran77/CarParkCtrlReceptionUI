<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane styleClass="background-pane" stylesheets="@../carpark-monitoring-view/carpark-monitoring-view.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.securitysystems.carparkctrlreceptionui.CarparkMonitoringController">
   <top>
      <HBox id="titlebar" maxHeight="50.0" prefHeight="50.0" spacing="30.0" styleClass="window-control-buttons-container" BorderPane.alignment="TOP_RIGHT">
         <Label styleClass="title-label" text="Carpark Monitoring Client" textFill="#f5f2f2">
            <font>
               <Font name="Ubuntu" size="20.0" />
            </font>
         </Label>
         <Region prefHeight="200.0" prefWidth="200.0" HBox.hgrow="ALWAYS" />
         <Button id="minimiseButton" ellipsisString="" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0">
            <font>
               <Font />
            </font>
            <styleClass>
               <String fx:value="minimise-button" />
               <String fx:value="window-control-button" />
               <String fx:value="button-style-set" />
            </styleClass>
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </Button>
         <Button id="maximiseButton" contentDisplay="CENTER" ellipsisString="" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0">
            <font>
               <Font />
            </font>
            <styleClass>
               <String fx:value="maximise-button" />
               <String fx:value="window-control-button" />
               <String fx:value="button-style-set" />
            </styleClass>
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </Button>
         <Button id="exitButton" ellipsisString="" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="20.0" minWidth="20.0" mnemonicParsing="false" prefHeight="20.0" prefWidth="20.0">
            <font>
               <Font />
            </font>
            <styleClass>
               <String fx:value="close-button" />
               <String fx:value="window-control-button" />
               <String fx:value="button-style-set" />
            </styleClass>
            <HBox.margin>
               <Insets />
            </HBox.margin>
         </Button>
      </HBox>
   </top>
   <center>
      <VBox styleClass="wrapper-pane" BorderPane.alignment="CENTER">
         <TabPane tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
            <Tab closable="false" text="Monitoring">
               <GridPane>
                  <columnConstraints>
                     <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" prefWidth="635.0"/>
                     <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" percentWidth="20"/>
                  </columnConstraints>
                  <rowConstraints>
                     <RowConstraints minHeight="10.0" percentHeight="35" vgrow="NEVER"/>
                     <RowConstraints minHeight="-Infinity" vgrow="ALWAYS"/>
                     <RowConstraints minHeight="10.0" percentHeight="10" vgrow="NEVER"/>
                  </rowConstraints>
                  <VBox GridPane.vgrow="NEVER">
                     <Label text="Last Numberplate" textFill="WHITE">
                        <VBox.margin>
                           <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                        </VBox.margin>
                        <font>
                           <Font name="System Bold" size="23.0"/>
                        </font>
                     </Label>
                     <HBox>
                        <Label prefWidth="93.0" text="TN57 DRT" textFill="WHITE">
                           <HBox.margin>
                              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                           </HBox.margin>
                        </Label>
                        <Label text="3 minutes ago" textFill="WHITE">
                           <HBox.margin>
                              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
                           </HBox.margin>
                        </Label>
                     </HBox>
                  </VBox>
                  <AnchorPane id="video-view-anchor" prefHeight="200.0" prefWidth="200.0" GridPane.rowIndex="1">
                     <ImageView pickOnBounds="true"/>
                  </AnchorPane>
                  <ScrollPane fitToHeight="true" hbarPolicy="NEVER" minWidth="-Infinity" GridPane.columnIndex="1"
                              GridPane.halignment="CENTER" GridPane.rowSpan="3">
                     <AnchorPane id="events-scroll-box" maxHeight="1.7976931348623157E308"
                                 maxWidth="1.7976931348623157E308">
                        <GridPane id="event-item">
                           <columnConstraints>
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                           </columnConstraints>
                           <rowConstraints>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                              <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
                           </rowConstraints>
                        </GridPane>
                     </AnchorPane>
                  </ScrollPane>
                  <GridPane GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="2"
                            GridPane.valignment="CENTER">
                     <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                     </columnConstraints>
                     <rowConstraints>
                        <RowConstraints minHeight="10.0" vgrow="SOMETIMES"/>
                     </rowConstraints>
                     <Button id="open-gate-button" alignment="CENTER" contentDisplay="CENTER"
                             maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                             mnemonicParsing="false" style="-fx-border-width: 3px;" text="Open Gate"
                             textOverrun="CLIP">
                        <padding>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                        </padding>
                        <styleClass>
                           <String fx:value="main-button-style-set"/>
                           <String fx:value="button-style-set"/>
                        </styleClass>
                     </Button>
                     <Button id="close-gate-button" alignment="CENTER" contentDisplay="CENTER"
                             maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                             mnemonicParsing="false" text="Close Gate" GridPane.columnIndex="1">
                        <padding>
                           <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                        </padding>
                        <styleClass>
                           <String fx:value="main-button-style-set"/>
                           <String fx:value="button-style-set"/>
                        </styleClass>
                     </Button>
                  </GridPane>
               </GridPane>
            </Tab>
            <Tab closable="false" text="Event Log">
               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </Tab>
            <Tab closable="false" text="Lists">
               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </Tab>
            <Tab closable="false" text="Other">
               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </Tab>
            <Tab closable="false" text="Settings">
               <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
            </Tab>
         </TabPane>
      </VBox>
   </center>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
</BorderPane>
